kops create cluster --name=kubepro.kunmidevops.com \ 
--state=s3://kops-test-bucket-1 --zones=eu-west-2a,eu-west-2b \ 
--node-count=2 --node-size=t3.small --master-size=t3.medium --dns-zone=kubepro.kunmidevops.com \ 
--node-volume-size=8 --master-volume-size=8

kops update cluster --name kubevpro.groophy.in --state=s3://vprofile-kop-states --yes --admin

kops delete cluster --name kubevpro.groophy.in --state=s3://vprofile-kop-states --yes --admin